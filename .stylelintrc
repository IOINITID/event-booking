{
  "extends": [
    "stylelint-config-htmlacademy",
    "stylelint-order-htmlacademy"
  ],
  "rules": {
    "declaration-no-important": true,
    "indentation": [
      2,
      {
        "ignore": [
          "inside-parens"
        ]
      }
    ],
    "string-quotes": "single",
    "no-eol-whitespace": null,
    "no-missing-end-of-source-newline": null
  }
}
